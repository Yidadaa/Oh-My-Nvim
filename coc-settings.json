{
  "cpp.formatting.provider": "clang-format",
  "coc.preferences.formatOnSaveFiletypes": [
    "python",
    "cpp",
    "json",
    "lua",
    "cmake",
    "vue",
    "typescript",
    "javascript"
  ],
  "git.addGBlameToVirtualText": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "Lua.telemetry.enable": true,
  "Lua.diagnostics.globals": ["vim"]
}
